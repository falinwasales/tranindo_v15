<odoo>
    <data>
        <record id="custom_account_journal_views" model="ir.ui.view">
            <field name="name">account.custom_account_journal_views</field>
            <field name="model">account.journal</field>
            <field name="inherit_id" ref="account.view_account_journal_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='sequence_id']" position="after">
                    <field name="ir_sequence_return_id"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>