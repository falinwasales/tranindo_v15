<odoo>
    <data>
        <record id="stock_landed_cost_custom" model="ir.ui.view">
            <field name="name">stock_landed_cost_custom</field>
            <field name="model">stock.landed.cost</field>
            <field name="inherit_id" ref="stock_landed_costs.view_stock_landed_cost_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='state']" position="after">
                    <field name="vendor_bill_id" optional="show"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>